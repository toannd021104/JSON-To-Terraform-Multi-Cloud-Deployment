{
  "instances": [
    {
      "name": "vm1",
      "image": "ubuntu-server-noble",
      "cpu": 1,
      "ram": 0.5,
      "disk": 5,
      "networks": [
        {
          "name": "net1",
          "ip": "192.168.10.10"
        }
      ],
      "keypair": "quoc-keypair",
      "security_groups": [
        "default"
      ],
      "floating_ip": false,
      "cloud-init": "ubuntu-user.json"
    }
  ],
  "networks": [
    {
      "name": "net1",
      "cidr": "192.168.10.0/24",
      "gateway_ip": "192.168.10.1",
      "enable_dhcp": true
    }
  ],
  "routers": [
    {
      "name": "router1",
      "external": true,
      "networks": [
        {
          "name": "net1",
          "ip": "192.168.10.1"
        }
      ]
    }
  ]
}